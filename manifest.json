{
    "name": "Stop Reels",
    "description": "Toggles on & off social media reels without removing other app functionalities.",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "controls.html"
    },
    "background": {
      "service_worker": "service-worker.js"
    },
    "permissions": [
      "proxy",
      "storage"
    ]
}